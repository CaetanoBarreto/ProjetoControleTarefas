@model IEnumerable<Tarefa>

<h2>Tarefas em Andamento</h2>

<table class="table">
    <thead>
        <tr>
            <th>Descrição</th>
            <th>Data de Início</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var tarefa in Model)
        {
            <tr>
                <td>@tarefa.Descricao</td>
                <td>@tarefa.DataInicio</td>
                <td>@tarefa.Status</td>
            </tr>
        }
    </tbody>
</table>
